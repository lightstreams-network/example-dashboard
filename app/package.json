{
  "name": "Lightstreams",
  "version": "0.0.1",
  "scripts": {
    "deploy": "gcloud app deploy app.yaml",
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "build:local": "export ASSET_PATH='/app'|| set ASSET_PATH='/app'&& npm run clean:dist && webpack --config config/webpack.local.js  --progress --profile --bail",
    "build:prod_node": "export ASSET_PATH='/app'|| set ASSET_PATH='/app'&& npm run clean:dist && webpack --config config/webpack.prod_node.js  --progress --profile --bail",
    "build:prod_node:debug": "export ASSET_PATH='/app'|| set ASSET_PATH='/app'&& npm run clean:dist && webpack --config config/webpack.prod_node_debug.js  --progress --profile --bail",
    "build:pre_prod_node": "npm run clean:dist && webpack --config config/webpack.prod_node.js  --progress --profile --bail",
    "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "ci:aot": "npm run lint && npm run test && npm run build:aot && npm run e2e",
    "ci:jit": "npm run lint && npm run test && npm run build:prod && npm run e2e",
    "ci:nobuild": "npm run lint && npm test && npm run e2e",
    "ci:testall": "npm run lint && npm run test && npm run build:prod && npm run e2e && npm run build:aot && npm run e2e",
    "ci:travis": "npm run lint && npm run test && npm run build:aot && npm run e2e:travis",
    "ci": "npm run ci:testall",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "docker": "docker",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "github-deploy:dev": "webpack --config config/webpack.github-deploy.js --progress --profile --env.githubDev",
    "github-deploy:prod": "webpack --config config/webpack.github-deploy.js --progress --profile --env.githubProd",
    "github-deploy": "npm run github-deploy:dev",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postinstall": "npm run build:prod",
    "postversion": "git push && git push --tags",
    "preclean:install": "npm run clean",
    "preversion": "npm test",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --port 3000 --progress --profile --watch --content-base src/",
    "server:local": "webpack-dev-server --config config/webpack.local.js --port 3000 --open --progress --profile --watch --content-base src/",
    "server:local:hmr": "npm run server:local -- --inline --hot",
    "server:remote": "webpack-dev-server --config config/webpack.remote.js --port 3000 --open --progress --profile --watch --content-base src/",
    "server:remote:hmr": "npm run server:remote -- --inline --hot",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "server": "npm run server:dev",
    "app-server:dev": "NODE_ENV=dev NODE_DEBUG=true PORT=3005 nodemon --watch 'src/server/**/*.*' --ignore 'src/server/**/*.spec.ts' --exec 'ts-node' src/server/main.js",
    "app-server:dev:debug": "NODE_ENV=dev NODE_DEBUG=true PORT=3005 nodemon --inspect --watch 'src/server/**/*.*' --ignore 'src/server/**/*.spec.ts' --exec 'ts-node' src/server/main.js",
    "start:hmr": "npm run server:dev:hmr",
    "start": "NODE_ENV=prod ts-node src/server/main.js",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "node-inspector": "node-inspector -p 8081",
    "node-inspector-debug": "DEBUG=node-inspector* node-inspector -p 8081"
  },
