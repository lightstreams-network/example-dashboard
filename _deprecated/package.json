    "luxon": "^1.1.0",
    "module-alias": "^2.1.0",
    "morgan": "~1.9.0",
    "nodemon": "^1.17.5",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "^4.37.6",
